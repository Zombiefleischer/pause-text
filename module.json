{
  "id": "pause-text",
  "title": "Alternative Pause Text",
  "description": "Set the pause message to a list of messages, whatever you prefer. Can be synced between players. You can also change the image. Supported formats are jpg, png, webp and gif. Forked from pause-icon.",
  "version": "3.0.0",
  "compatibility": {
    "minimum": 13,
    "verified": 13
  },
  "authors": [
    {
      "name": "Zombiefleischer",
      "url": "https://github.com/Zombiefleischer/pause-text",
      "email": "zombiefleischer@zombiefleischer.de",
      "discord": "zombiefleischer",
      "flags": {
        "github": "Zombiefleischer"
      }
    }
  ],
  "library": false,
  "socket": true,
  "relationships": {
    "systems": [],
    "requires": [
      {
        "id": "socketlib",
        "type": "module",
        "reason": "For the added sync functionality.",
        "compatibility": {}
      }
    ],
    "recommends": [],
    "conflicts": [
      {
        "id": "pause-icon",
        "type": "module",
        "manifest": "https://gitlab.com/Freeze020/pause-icon/-/raw/master/module.json",
        "reason": "It's the original module with a bit less features.",
        "compatibility": {}
      }
    ]
  },
  "esmodules": ["scripts/messages.js", "scripts/main.js"],
  "styles": ["styles/pt-config.css"],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "languages/en.json"
    },
    {
      "lang": "de",
      "name": "Deutsch",
      "path": "languages/de.json"
    },
    {
      "lang": "es",
      "name": "Spanish",
      "path": "languages/es.json"
    },
    {
      "lang": "fr",
      "name": "Fran&ccedil;ais",
      "path": "languages/fr.json"
    }
  ],
  "url": "https://github.com/Zombiefleischer/pause-text",
  "manifest": "",
  "download": "",
  "readme": "https://raw.githubusercontent.com/Zombiefleischer/pause-text/master/README.md",
  "license": "https://raw.githubusercontent.com/Zombiefleischer/pause-text/master/LICENSE",
  "bugs": "https://github.com/Zombiefleischer/pause-text/issues"
}
